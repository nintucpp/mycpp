TOP=../..
CURRENT=include/omnithread
BASE_OMNI_TREE=../..

INSTALL=/usr/bin/install -c

INSTALLDIR = $(INSTALLINCDIR)/omnithread

HEADERS = mach.h nt.h posix.h pthread_nt.h solaris.h

include $(TOP)/mk/beforeauto.mk

install:: $(HEADERS)
	@(dir="$(INSTALLDIR)"; \
          for file in $^; do \
            $(ExportFileToDir) \
          done )

include $(TOP)/mk/afterauto.mk
